Output: vector s of spans of the vector x
\end_layout

\begin_layout LyX-Code
for i = 0 to n-1 do
\end_layout

\begin_layout LyX-Code
   j = 1
\end_layout

\begin_layout LyX-Code
   while (j <= i 
\begin_inset Formula $\wedge$
\end_inset

 x[i-j] <= x[i])
\end_layout

\begin_layout LyX-Code
      j =  j + 1
\end_layout

\begin_layout LyX-Code
   s[i] = j
\end_layout

\begin_layout LyX-Code
return s
\end_layout

